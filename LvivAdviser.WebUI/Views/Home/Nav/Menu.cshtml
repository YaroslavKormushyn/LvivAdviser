@model IEnumerable<string>
@Html.ActionLink("Home", "List", "Content", null, new { @class = "btn btn-block btn-default btn-lg" })
@foreach (var link in Model)
{
    @Html.RouteLink(link, new {
        controller = "Content",
        action = "List",
        type = link,
        page = 1
    }, new
    {
        @class = "btn btn-block btn-default btn-lg"
    })
}
